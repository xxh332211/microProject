page {
  height: 100%;
  background: #FF625D;
}
.activity {
  width: 750rpx;
  background: #fff;
  border-radius: 30rpx 30rpx 0 0;
  box-sizing: border-box;
  padding-top: 40rpx;
}
.activity .tab {
  display: flex;
  box-sizing: border-box;
  justify-content: space-around;
  margin-bottom: 20rpx;
}
.activity .tab .single {
  width: 200rpx;
  height: 84rpx;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.activity .tab .single.selected .line {
  display: block;
}
.activity .tab .single .line {
  width: 40rpx;
  height: 4rpx;
  background: #FF5175;
  opacity: 1;
  position: absolute;
  bottom: 6rpx;
  left: 50%;
  transform: translateX(-24rpx);
  display: none;
}
.activity .tab .single text {
  font-size: 32rpx;
  font-family: 'PingFang';
  color: #858FAD;
}
.activity .forms {
  width: 673rpx;
  background: #F5F6FA;
  border: 2px solid #F5F6FA;
  border-radius: 30rpx;
  margin: 0 auto;
  padding: 32rpx 40rpx;
  box-sizing: border-box;
}
.activity .forms .single {
  margin-bottom: 50rpx;
}
.activity .forms .single .dateBox {
  display: flex;
  justify-content: flex-start;
}
.activity .forms .single .dateBox picker {
  margin-right: 20rpx;
}
.activity .forms .inputtitle {
  font-size: 32rpx;
  font-weight: 700;
  color: #3E455A;
  margin-bottom: 20rpx;
}
.activity .forms .input_box {
  height: 80rpx;
  background: #FFFFFF;
  border: 2rpx solid #DDE1ED;
  opacity: 1;
  border-radius: 30rpx;
}
.activity .forms .input_box .input_label::before {
  content: '' !important;
  display: none !important;
}
.activity .forms .textbox {
  width: 592rpx;
  height: 200rpx;
  background: #FFFFFF;
  border: 2rpx solid #DDE1ED;
  opacity: 1;
  border-radius: 30rpx;
}
.activity .forms .picker {
  border-radius: 20rpx;
  overflow: hidden;
  margin-right: 30rpx;
  background: #fff;
}
.activity .forms .picker-index--l-grid-item {
  margin-bottom: 30rpx;
}
.activity .forms .picker-index--min-img {
  margin: 0 !important;
}
.activity .forms .picker_date {
  display: inline-block;
  padding: 10rpx 20rpx;
  background: #FF5175;
  color: #fff;
  border-radius: 10rpx;
}
.activity .forms .submitbtn {
  margin-top: 60rpx;
  margin-bottom: 30rpx;
  width: 590rpx;
}
.activity .forms .submitbtn .btn {
  width: 590rpx;
  height: 84rpx;
  background: #FF5175;
  opacity: 1;
  border-radius: 30rpx;
  color: #fff;
  font-size: 32rpx;
}
