}
.red .main {
  background: #fff;
  border-radius: 15px 15px 0 0;
  padding-top: 20rpx;
  width: 750rpx;
  padding-left: 40rpx;
  padding-right: 38rpx;
  box-sizing: border-box;
  padding-bottom: 40rpx;
}
.red .main .main_title {
  color: #3E455A;
  font-size: 48rpx;
  font-weight: 500;
}
.red .main .form {
  background: #F5F6FA;
  padding: 40rpx;
  box-sizing: border-box;
  width: 100%;
  border-radius: 10px;
  margin-top: 30rpx;
}
.red .main .form .belong {
  font-size: 36rpx;
  color: #3E455A;
  padding-bottom: 32rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #DDE1ED;
  margin-bottom: 32rpx;
}
.red .main .form .info {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.red .main .form .info .user {
  padding: 12rpx 28rpx;
  background: #fff;
  border-radius: 30px;
  box-sizing: border-box;
  display: inline-flex;
  justify-content: center;
  align-items: center;
}
.red .main .form .info .user text {
  margin-left: 20rpx;
  color: #3E455A;
  font-size: 24rpx;
}
.red .main .form .info .time {
  color: #858FAD;
  font-size: 24rpx;
}
.red .main .form .detail .title {
  color: #3E455A;
  font-size: 32rpx;
  margin-top: 32rpx;
}
.red .main .form .detail .imgs {
  margin-top: 20rpx;
  display: flex;
  flex-wrap: wrap;
}
.red .main .form .detail .imgs .img {
  width: 184rpx;
  height: 184rpx;
  background: #ccc;
  margin-bottom: 20rpx;
  border-radius: 10px;
  overflow: hidden;
}
.red .main .form .detail .imgs .img:nth-child(3n-1) {
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.red .main .form .detail .imgs .img image {
  width: 100%;
  height: 100%;
}
.red .main .form .up {
  margin-top: 20rpx;
}
.red .main .form .up .text {
  background: #fff;
  border-radius: 5px;
  border: 2rpx solid #DDE1ED;
  font-size: 28rpx;
  color: #3E455A;
  margin-bottom: 40rpx;
  box-sizing: border-box;
}
.red .main .form .up .picker_c {
  margin-left: -8rpx;
  width: 30%;
}
.red .main .form .up .picker {
  overflow: hidden;
  width: 184rpx;
  height: 184rpx;
  background: #FFFFFF;
  border-radius: 20rpx;
  margin-bottom: 40rpx;
  box-sizing: border-box;
}
.red .main .form .btn {
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.red .main .form .btn .big_button {
  background: #FF5175;
  width: 590rpx;
  height: 84rpx;
  font-size: 32rpx;
  border-radius: 10px;
}
.red .main .form .btn .big_button.disabled {
  background: #DDE1ED;
  color: #FFFFFF;
}
.red .main .form .btn .little_button {
  width: 76rpx;
  height: 50rpx;
  background: #F4E9EB;
  opacity: 1;
  border-radius: 20px;
  color: #FF5175;
  font-size: 24rpx;
}
