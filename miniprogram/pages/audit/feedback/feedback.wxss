.default {
  color: #333333 !important;
}
.white_f {
  color: #fff !important;
}
.maincolor_c {
  color: #34bfa3 !important;
}
.mainred_c {
  color: #FF625D !important;
}
.maincolor_b {
  background: #34bfa3 !important;
}
.gray {
  color: #858FAD !important;
}
page {
  height: 100%;
}
.main {
  background: #FF625D;
  height: 100%;
}
.main .content {
  background: #fff;
  border-radius: 15px 15px 0 0;
  padding-top: 20rpx;
  height: 100%;
}
.main .content .tab {
  display: flex;
  box-sizing: border-box;
  justify-content: space-around;
}
.main .content .tab .single {
  width: 200rpx;
  height: 84rpx;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.main .content .tab .single.selected .line {
  display: block;
}
.main .content .tab .single .line {
  width: 40rpx;
  height: 4rpx;
  background: #FF5175;
  opacity: 1;
  position: absolute;
  bottom: 6rpx;
  left: 50%;
  transform: translateX(-24rpx);
  display: none;
}
.main .content .tab .single text {
  font-size: 32rpx;
  font-family: 'PingFang';
  color: #858FAD;
}
.main .content .search {
  margin-top: 30rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.main .content .search .dateBox {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
}
.main .content .search .dateBox .picker_date .input_box {
  height: 60rpx;
  background: #FFFFFF;
  border: 2rpx solid #DDE1ED;
  opacity: 1;
  border-radius: 30rpx;
}
.main .content .search .dateBox .picker_date .input_box .input_label::before {
  content: '' !important;
  display: none !important;
}
.main .content .list {
  padding-left: 39rpx;
  padding-right: 39rpx;
  box-sizing: border-box;
  overflow: hidden;
  margin-top: 30rpx;
}
.main .content .list .main_title {
  color: #3E455A;
  font-size: 48rpx;
  font-weight: 500;
}
.main .content .list .single {
  background: #F5F6FA;
  padding: 40rpx;
  box-sizing: border-box;
  width: 100%;
  border-radius: 10px;
  margin-top: 30rpx;
}
.main .content .list .single .belong {
  font-size: 36rpx;
  color: #3E455A;
  padding-bottom: 32rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #DDE1ED;
  margin-bottom: 32rpx;
}
.main .content .list .single .info {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.main .content .list .single .info .user {
  padding: 12rpx 28rpx;
  background: #fff;
  border-radius: 30px;
  box-sizing: border-box;
  display: inline-flex;
  justify-content: center;
  align-items: center;
}
.main .content .list .single .info .user text {
  margin-left: 20rpx;
  color: #3E455A;
  font-size: 24rpx;
}
.main .content .list .single .info .time {
  color: #858FAD;
  font-size: 24rpx;
}
.main .content .list .single .detail {
  margin-top: 32rpx;
}
.main .content .list .single .detail .title {
  color: #3E455A;
  font-size: 32rpx;
}
.main .content .list .single .detail .imgs {
  margin-top: 20rpx;
  display: flex;
  flex-wrap: wrap;
}
.main .content .list .single .detail .imgs .img {
  width: 184rpx;
  height: 184rpx;
  background: #ccc;
  margin-bottom: 20rpx;
  border-radius: 10px;
  overflow: hidden;
}
.main .content .list .single .detail .imgs .img:nth-child(3n-1) {
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.main .content .list .single .detail .imgs .img image {
  width: 100%;
  height: 100%;
}
.main .content .list .single .btn {
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.main .content .list .single .btn .big_button {
  background: #FF5175 !important;
  width: 590rpx;
  height: 84rpx;
  font-size: 32rpx;
  border-radius: 20px;
}
.main .content .list .single .btn .little_button {
  width: 76rpx;
  height: 50rpx;
  background: #F4E9EB;
  opacity: 1;
  border-radius: 20px;
  color: #FF5175;
  font-size: 24rpx;
}
